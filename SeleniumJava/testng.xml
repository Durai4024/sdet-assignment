<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Autify Test Suite" verbose="1">


	<test name="Autify Smoke Test">

		<groups>
			<run>
				<include name="Smoke"></include>
				<include name="Sanity"></include>
			</run>
		</groups>

		<classes>
			<class name="com.autify.qa.SeleniumJava.AutifyTest" />
		</classes>
	</test>
</suite>